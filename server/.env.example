# Server
SERVER_PORT=8080

# Database
DB_HOST=127.0.0.1
DB_PORT=3306
DB_USER=root
DB_PASSWORD=your_password
DB_NAME=pinche

# Redis
REDIS_HOST=127.0.0.1
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# JWT
JWT_SECRET=your-jwt-secret-key
JWT_EXPIRE_HOUR=168

# Tencent COS
COS_SECRET_ID=your_cos_secret_id
COS_SECRET_KEY=your_cos_secret_key
COS_BUCKET=your_bucket_name
COS_REGION=ap-shanghai

# 日志配置
LOG_LEVEL=info          # 日志级别 debug/info/warn/error
LOG_FILENAME=logs/app.log  # 日志文件路径
LOG_MAX_SIZE=200        # 单文件最大MB
LOG_MAX_BACKUPS=7       # 保留文件数
LOG_MAX_AGE=30          # 保留天数
LOG_COMPRESS=true       # 是否压缩
LOG_CONSOLE=true        # 是否输出到控制台
